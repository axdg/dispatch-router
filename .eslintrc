{
    "extends": "airbnb-base",
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true,
        "mocha": true
    },
    "ecmaFeatures": {
        "forOf": true,
        "es6": true
    },
    "rules": {
        "semi": [ "error", "never" ],
        "no-param-reassign": "off",
        "no-unused-expressions": "off",
        "prefer-arrow-callback": "off",
        "no-underscore-dangle": "off"
    }
}
